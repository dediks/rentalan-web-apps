@*@using PakMua.Models

@model ListSearchMobil*@

<div class="card">
    <div class="card-header">
        <h5>Filter </h5>
    </div>
    <div class="card-body">
        <div class="form-group">
            <label for="nama-mobil">Nama Mobil</label>
            <input name="nama" type="text" id="nama" class="form-control"> 
        </div>
        <div class="form-group">
            <label for="brand">Brand</label>

            <select id="brand" name="brand" class="form-control form-control-lg">
                <option value="">Pilih Brand</option>
                <option value="Toyota">Toyota</option>
                <option value="Honda">Honda</option>
                <option value="Mitshubishi">Mitshubishi</option>
                <option value="Daihatsu">Daihatsu</option>
                <option value="Suzuki">Suzuki</option>
                <option value="Nissan">Nissan</option>
                <option value="Datsun">Datsun</option>
                <option value="Wuling">Wuling</option>
            </select>
        </div>
        <div class="form-group">
            <button type="button" id="btn-cari" class="btn btn-primary float-right">Cari</button>
        </div>
    </div>
    <div class="card-header mt-2">
        <h5>Tanggal </h5>
    </div>
    <div class="card-body d-flex d-flex-between">
        <div class="d-block">
            <div class="col">Ambil</div>
            <div class="col">
                <span id="el-tgl-ambil"></span>
            </div>
        </div>
        <div class="d-block">
            <div class="col">Kembali</div>
            <div class="col">
                <span id="el-tgl-kembali"></span>
            </div>
        </div>
    </div>
</div>

