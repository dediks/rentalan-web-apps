@model IEnumerable<PakMua.tb_pengembalian>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.tgl_pengembalian)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.denda)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tb_order.no_invoice)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.tgl_pengembalian)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.denda)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tb_order.no_invoice)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id_pengembalian }) |
            @Html.ActionLink("Details", "Details", new { id=item.id_pengembalian }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id_pengembalian })
        </td>
    </tr>
}

</table>
